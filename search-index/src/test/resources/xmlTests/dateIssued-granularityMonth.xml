            <field name="date_range_start.year">1952</field>

            <!--
            &lt;!&ndash;spatne&ndash;&gt;
            <field name="datum">1951-01-06T23:00:00.000Z</field>
            <field name="datum_str">7.-11.1952</field>
            &lt;!&ndash;spatne&ndash;&gt;
