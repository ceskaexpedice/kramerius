<testSuite testType="mods" model="monograph">

    <test name="Jaro 2012">
        <desc>Jaro roku (meteorogicky: březen-květen, ne astronomicky)</desc>
        <in>
            <originInfo>
                <dateIssued>Jaro 2012</dateIssued>
            </originInfo>
        </in>
        <out>
            <field name="date.max">2012-05-31T23:59:59.999Z</field>
            <field name="date.min">2012-03-01T00:00:00.001Z</field>
            <field name="date.str">Jaro 2012</field>
            <field name="date_range_end.day">31</field>
            <field name="date_range_end.month">5</field>
            <field name="date_range_end.year">2012</field>
            <field name="date_range_start.day">1</field>
            <field name="date_range_start.month">3</field>
            <field name="date_range_start.year">2012</field>
        </out>
    </test>

